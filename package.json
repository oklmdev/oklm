{
  "name": "starter-betagouv-template",
  "version": "0.0.1",
  "description": "Cr√©er une landing page avec DSFR et Netlify CMS",
  "keywords": [],
  "scripts": {
    "build": "tsc && cp -r src/assets dist && yarn build:front",
    "build:front": "webpack --config=webpack.config.js",
    "start": "node dist/index.js",
    "watch:front": "webpack --config=webpack.config.js --watch",
    "watch": "tsc && cp -r src/assets dist && concurrently \"tsc -w\" \"yarn run start\" \"yarn watch:front\"",
    "migrate": "ts-node ./src/migrate.ts",
    "test": "jest --runInBand",
    "test:unit": "jest --testRegex='^(().)*.spec.ts$' --runInBand",
    "test:watch": "jest --runInBand --watch",
    "db": "docker compose down --remove-orphans && docker compose up -d",
    "db:test": "export NODE_ENV=test && export COMPOSE_PROJECT_NAME=oklm_tests && docker compose down --remove-orphans && docker compose -f ./docker-compose-integration.yml up -d && until docker exec oklm_db_tests pg_isready -U test -d oklm_test; do sleep 1; done && yarn migrate",
    "lint": "eslint --config=./.tooling/.eslint/.eslintrc.cjs  ./src/",
    "prettier": "prettier --config=./.tooling/.prettier/.prettierrc.cjs  --write  ./src/",
    "prettier:check": "prettier --config=./.tooling/.prettier/.prettierrc.cjs --check ./src/",
    "start:dev": "nodemon dist/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "homepage": "https://github.com/oklmdev/starter-betagouv-template#readme",
  "bugs": {
    "url": "https://github.com/oklmdev/starter-betagouv-template/issues"
  },
  "license": "MIT",
  "contributors": [
    "Romain Cambonie <romain.cambonie@gmail.com> (https://github.com/romain-cambonie)",
    "Pierre-Antoine Duchateau <pad@dabla.com> (https://github.com/paduc)",
    "Marc Gavanier <marc.gavanier@gmail.com> (https://github.com/marc-gavanier)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oklmdev/starter-betagouv-template.git"
  },
  "main": "src/index.ts",
  "bin": "init.js",
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "@dataesr/react-dsfr": "^2.3.1",
    "@gouvfr/dsfr": "^1.4.0",
    "callsites": "3.1.0",
    "dotenv": "^16.0.0",
    "esbuild-loader": "^2.19.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "glob": "^8.0.3",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "zod": "^3.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.0",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.4",
    "@babel/core": "^7.17.10",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.0.0",
    "jest": "^27.4.7",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.7.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
