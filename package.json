{
  "name": "starter-betagouv",
  "version": "0.0.2",
  "description": "Un code sain pour un dev serein",
  "keywords": [],
  "scripts": {
    "start": "nodemon dist/index.js",
    "watch": "tsc && cp -r src/assets dist && concurrently \"tsc -w\" \"yarn run start\"",
    "prettier": "prettier --write . --ignore-unknown",
    "migrate": "ts-node ./src/migrate.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint -c ./eslintrc.js src",
    "db": "docker compose down --remove-orphans && docker compose up -d",
    "db:test": "export NODE_ENV=test && export COMPOSE_PROJECT_NAME=oklm_tests && docker compose down --remove-orphans && docker compose -f ./docker-compose-integration.yml up -d && until docker exec oklm_db_tests pg_isready -U test -d oklm_test; do sleep 1; done && yarn migrate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "homepage": "https://github.com/oklmdev/oklm#readme",
  "bugs": {
    "url": "https://github.com/oklmdev/oklm/issues"
  },
  "license": "MIT",
  "contributors": [
    "Romain Cambonie <romain.cambonie@gmail.com> (https://github.com/romain-cambonie)",
    "Pierre-Antoine Duchateau <pad@dabla.com> (https://github.com/paduc)",
    "Marc Gavanier <marc.gavanier@gmail.com> (https://github.com/marc-gavanier)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oklmdev/oklm.git"
  },
  "main": "src/index.ts",
  "bin": "init.js",
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "@dataesr/react-dsfr": "^2.3.1",
    "@gouvfr/dsfr": "^1.4.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.0",
    "@types/pg": "^8.6.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.2",
    "keycloak-connect": "^17.0.1",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "zod": "^3.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-loader": "^8.2.5",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "yarn": "^1.22.17"
  }
}
